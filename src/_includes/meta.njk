<meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta
      name="description" content="{{ description | safe}}">
      <!-- Google tags -->
      <meta itemprop="name" content="loopdash">
        <meta itemprop="description" content="{{ description | safe}}">
          <meta
            itemprop="image" content="">
            <!-- Facebook tags -->
            <meta property="og:url" content="{{ page.url | safe }}">
              <meta property="og:type" content="website">
                <meta property="og:title" content="{{ h1 | safe }}">
                  <meta property="og:description" content="{{ description | safe}}">
                    <meta
                      property="og:image" content="../../images/meta.png">
                      <!-- Twitter tags -->
                      <meta name="twitter:card" content="summary">
                        <meta name="twitter:title" content="{{ h1 | safe }}">
                          <meta name="twitter:description" content="{{ description | safe}}">
                            <meta name="twitter:image" property="og:image" content="../../images/meta.png">
                              <meta name="robots" content="index, follow"/>
                              <meta name="googlebot" content="index, follow"/>