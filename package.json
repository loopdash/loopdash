{
  "name": "y",
  "version": "1.0.0",
  "description": "Loopdash base WordPress theme",
  "main": "index.js",
  "scripts": {
    "compile": "node-sass -w -r scss/ -o dist/",
    "prefix": "postcss dist/style.css --use=autoprefixer --output=style.css",
    "start": "nodemon -e scss -w scss -x \"npm run compile && npm run prefix\"",
    "dev": "webpack --mode development",
    "build": "postcss style.css --output=dist/style.css "
