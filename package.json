{
  "name": "loopdash",
  "version": "1.0.0",
  "description": "",
  "main": ".eleventy.js",
  "scripts": {
    "start": "run-p start:*",
    "start:eleventy": "npx @11ty/eleventy --serve",
    "start:postcss": "postcss src/styles/*.css --dir _site/css --watch",
    "start:watch": "sass  --no-source-map --watch src/scss:_site/css",
    "dev": "npm-run-all --parallel start watch",
    "build": "npx @11ty/eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loopdash/loopdash.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/loopdash/loopdash/issues"
  },
  "homepage": "https://github.com/loopdash/loopdash#readme",
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-img": "^0.9.0",
    "@quasibit/eleventy-plugin-sitemap": "^2.1.4",
    "autoprefixer": "^10.3.1",
    "cssnano": "^5.0.6",
    "postcss": "^8.3.5",
    "postcss-cli": "^8.3.1",
    "postcss-nested": "^5.0.5",
    "sass": "^1.35.1",
    "tailwindcss": "^2.2.4"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
